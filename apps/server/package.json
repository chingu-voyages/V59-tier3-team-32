    "pg": "8.17.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.51.4",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/pg": "8.16.0",
    "drizzle-kit": "0.31.8",
    "nodemon": "3.1.11",
    "ts-node": "10.9.2"
  }
}