{
  "name": "v59-tier3-team-32",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "a web based interview question generator.",
  "license": "AGPL-3.0-or-later",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "prepare": "husky",
    "lint": "eslint apps",
    "======================= WEB ========================": "",
    "web:dev": "npm run dev -w apps/web",
    "web:build": "npm run build -w apps/web",
    "web:start": "npm run start -w apps/web",
    "===================== SERVER =======================": "",
    "server:dev": "npm run dev -w apps/server",
    "server:build": "npm run build -w apps/server",
    "server:start": "npm run start -w apps/server",
    "======================= DB =========================": "",
    "db:generate": "npm run db:generate -w apps/server",
    "db:migrate": "npm run db:migrate -w apps/server",
    "======================= UNIT TESTS =========================": "",
    "test": "vitest",
    "test:run": "vitest run",
    "test:web": "vitest apps/web",
    "test:server": "vitest apps/server"
  },
  "lint-staged": {
    "apps/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "vitest run --passWithNoTests"
    ]
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chingu-voyages/V59-tier3-team-32.git"
  },
  "homepage": "https://github.com/chingu-voyages/V59-tier3-team-32#readme",
  "contributors": [
    "See contributors at https://github.com/chingu-voyages/v59-tier3-team-32/graphs/contributors"
  ],
  "devDependencies": {
    "@typescript-eslint/parser": "8.46.0",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "vitest": "4.0.18"
  }
}